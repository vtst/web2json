{
  "name": "Web2JSON",
  "version": "1.0",
  "description": "Extract information from the web into JSON",
  "manifest_version": 2,
  "background": {
    "scripts": ["utils.js", "background.js"],
    "persistent": false
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "contextMenus",
    "tabs"
  ]
}
