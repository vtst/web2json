{
  "name": "Web2JSON",
  "version": "1.0",
  "description": "Extract information from the web into JSON",
  "devtools_page": "devtools/page.html",
  "manifest_version": 2,
  "background": {
    "scripts": ["utils.js", "engine.js", "localeval.js", "background.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ajax.googleapis.com; object-src 'self'",
  "permissions": [
    "http://*/",
    "https://*/",
    "file://*",
    "contextMenus",
    "tabs"
  ]
}
